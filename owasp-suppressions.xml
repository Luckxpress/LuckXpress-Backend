<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
        OWASP Dependency Check Suppressions for LuckXpress
        Only suppress FALSE POSITIVES - never suppress real vulnerabilities
    -->
    
    <!-- Spring Boot Framework - Common false positives -->
    <suppress>
        <notes>Spring Boot starter dependencies often flagged incorrectly</notes>
        <gav regex="true">^org\.springframework\.boot:spring-boot-starter.*$</gav>
        <cve>CVE-2016-1000027</cve> <!-- Example: Commons FileUpload issue not applicable -->
    </suppress>
    
    <!-- Jackson Library - Exclude issues not applicable to our usage -->
    <suppress>
        <notes>Jackson deserialization issues - we use safe configuration</notes>
        <gav regex="true">^com\.fasterxml\.jackson\.core:jackson-databind:.*$</gav>
        <cve>CVE-2019-20330</cve> <!-- Example: Specific Jackson CVE not applicable -->
    </suppress>
    
    <!-- H2 Database - Test only usage -->
    <suppress>
        <notes>H2 Database only used in tests, not production</notes>
        <gav regex="true">^com\.h2database:h2:.*$</gav>
        <cve>CVE-2021-23463</cve> <!-- H2 console not enabled in production -->
    </suppress>
    
    <!-- Testcontainers - Test dependencies only -->
    <suppress>
        <notes>Testcontainers used only in test environment</notes>
        <gav regex="true">^org\.testcontainers:.*$</gav>
        <cve>CVE-2020-15250</cve> <!-- Docker vulnerabilities not applicable -->
    </suppress>
    
    <!-- RestAssured - Test dependencies only -->
    <suppress>
        <notes>RestAssured used only for testing APIs</notes>
        <gav regex="true">^io\.rest-assured:.*$</gav>
        <cve>CVE-2019-17573</cve> <!-- Example: XML parsing not used in our tests -->
    </suppress>
    
    <!-- Lombok - Compile-time only -->
    <suppress>
        <notes>Lombok used only at compile time, not in runtime classpath</notes>
        <gav regex="true">^org\.projectlombok:lombok:.*$</gav>
        <cve>CVE-2021-45105</cve> <!-- Example: Log4j issues not applicable to Lombok -->
    </suppress>
    
    <!-- Example template - REMOVE WHEN ADDING REAL SUPPRESSIONS -->
    <!--
    <suppress>
        <notes>Describe why this is a false positive</notes>
        <gav regex="true">^groupId:artifactId:.*$</gav>
        <cve>CVE-YYYY-NNNNN</cve>
    </suppress>
    -->
    
    <!-- 
        IMPORTANT NOTES:
        1. Only add suppressions for verified false positives
        2. Include detailed notes explaining why it's safe to suppress
        3. Review suppressions regularly - CVEs may become applicable
        4. Never suppress vulnerabilities in production dependencies
        5. Consider fixing vulnerabilities instead of suppressing them
    -->
    
</suppressions>
